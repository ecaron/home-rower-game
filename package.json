{
  "name": "home-rower-game",
  "version": "2.0.0",
  "description": "A rowing machine webapp for friendly in-house competitions",
  "scripts": {
    "start": "node app/main",
    "estart": "electron .",
    "install": "electron-rebuild",
    "lint": "standard --fix",
    "dev": "nodemon -e js,css,njk --ignore './public/' app/main",
    "postinstall": "electron-builder install-app-deps"
  },
  "main": "electron.js",
  "license": "MIT",
  "repository": "ecaron/home-rower-game",
  "engines": {
    "node": ">=12"
  },
  "author": {
    "name": "Eric Caron",
    "url": "https://github.com/ecaron"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "bootstrap": "^5.0.1",
    "bootstrap-icons": "^1.5.0",
    "chart.js": "^3.0.2",
    "connect-session-sequelize": "^7.1.1",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "get-port": "^5.1.1",
    "jquery": "^3.6.0",
    "luxon": "^1.27.0",
    "nosleep.js": "^0.12.0",
    "nunjucks": "^3.2.3",
    "sequelize": "^6.6.2",
    "serialport": "^9.0.7",
    "sqlite3": "^5.0.2",
    "umzug": "^2.3.0",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "electron": "^13.1.2",
    "electron-builder": "^22.11.7",
    "electron-rebuild": "^2.3.5",
    "node-abi": "^2.30.0",
    "nodemon": "^2.0.7",
    "prebuild-install": "^6.1.3",
    "sequelize-cli": "^6.2.0",
    "standard": "^16.0.3"
  },
  "build": {
    "appId": "com.ericcaron.homerower",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  }
}
